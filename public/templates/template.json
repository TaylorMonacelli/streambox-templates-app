{
    "template": {
        "darkMode": true,
        "navbar": {
            "routes": [
                {
                    "routeName": "Info",
                    "containersStyle": {
                        "numberOfColumns": 2
                    },
                    "containers": [
                        {
                            "title": "Video",
                            "apiSrc": "/REST/encoder/status",
                            "type": "readonly",
                            "containerStyle": {
                                "stretchVertically": 2
                            },
                            "fields": [
                                {
                                    "type": "video",
                                    "previewImageRoute": "/REST/encoder/preview/JPG"
                                },
                                {
                                    "type": "text",
                                    "label": "Source Video Format",
                                    "mapping": "inputsignal"
                                }, 
                                {
                                    "type": "text",
                                    "label": "Network Mode",
                                    "mapping": "cnetworkmode"
                                },
                                {
                                    "type": "text",
                                    "label": "Bitrate",
                                    "mapping": "cur_bitrate"
                                },
                                {
                                    "type": "text",
                                    "label": "Quality",
                                    "mapping": "vquality"
                                },
                                {
                                    "type": "boolean",
                                    "label": "Is Streaming?",
                                    "mapping": "isStreaming"
                                },
                                {
                                    "type": "text",
                                    "label": "Dropped Captured Frames",
                                    "mapping": "dropCapture"
                                },
                                {
                                    "type": "text",
                                    "label": "Dropped Encoder Frames",
                                    "mapping": "dropEnc"
                                },
                                {
                                    "type": "text",
                                    "label": "SDI errors",
                                    "mapping": "sdi_error"
                                },
                                {
                                    "type": "text",
                                    "label": "Frame Delay",
                                    "mapping": "frame_delay"
                                }
                            ]
                        },
                        {
                            "title": "Network",
                            "apiSrc": "/REST/encoder/status",
                            "backgroundColor": "#C7D3D4FF",
                            "fields": [
                                {
                                    "type": "text",
                                    "label": "Encoder IP",
                                    "mapping": "c_encoderIP"
                                },
                                {
                                    "type": "text",
                                    "label": "Decoder IP",
                                    "mapping": "c_decoderIP"
                                },
                                {
                                    "type": "text",
                                    "label": "Packet Size",
                                    "mapping": "c_packetsize"
                                },
                                {
                                    "type": "text",
                                    "label": "Shuffle",
                                    "mapping": "cshuffle"
                                },{
                                    "type": "text",
                                    "label": "Buffer",
                                    "mapping": "cbuffer"
                                },
                                {
                                    "type": "text",
                                    "label": "Bytes",
                                    "mapping": "cbytes"
                                },
                                {
                                    "type": "text",
                                    "label": "Packets Sent",
                                    "mapping": "sent_packets"
                                },
                                {
                                    "type": "text",
                                    "label": "Transmission",
                                    "mapping": "is_ldmp"
                                }
                            ]
                        },
                        {
                            "title": "Start Streaming",
                            "apiSrc": "/REST/encoder/status",
                            "backgroundColor": "rgb(250, 208, 201)",
                            "fields": [
                                {
                                    "type": "button",
                                    "size": "giant",
                                    "mapping": "isStreaming",
                                    "action": "toggleStreaming"
                                }
                            ]
                        }
                    ]
                },
                {
                    "routeName": "Network",
                    "containersStyle": {
                        "numberOfColumns": 2
                    },
                    "containers": [
                        {
                            "title": "Bitrate and Buffer Settings",
                            "apiSrc": ["/REST/encoder/network", "/REST/encoder/video"],
                            "type": "form",
                            "postEndpoint": "/REST/encoder/video",
                            "backgroundColor": "#EEF3E2",
                            "fields": [
                                {
                                    "type": "input",
                                    "label": "Target Bitrate",
                                    "mapping": "TargetBitrate",
                                    "endLabel": "kbps"
                                },
                                {
                                    "type": "input",
                                    "label": "VBR Buffer",
                                    "mapping": "VBR",
                                    "endLabel": "seconds"
                                },
                                {
                                    "type": "checkbox",
                                    "label": "Packet Stuffing",
                                    "mapping": "stuffing"
                                },
                                {
                                    "type": "button",
                                    "action": "submitLocalForm",
                                    "size": "small",
                                    "label": "Apply Changes"
                                }
                            ]
                        },
                        {
                            "title": "Transport Settings",
                            "apiSrc": "/REST/encoder/transport",
                            "backgroundColor": "#f6d8ac",
                            "postEndpoint": "/REST/encoder/transport",
                            "type": "form",
                            "containerStyle": {
                                "stretchVertically": 2
                            },
                            "fields": [
                                {
                                    "type": "select",
                                    "label": "Protocol",
                                    "mapping": "ldmp-use"
                                },
                                {
                                    "type": "input",
                                    "label": "CWND",
                                    "mapping": "ldmp-cwnd"
                                },
                                {
                                    "type": "input",
                                    "label": "CWND MIN",
                                    "mapping": "ldmp-cwndmin"
                                },
                                {
                                    "type": "input",
                                    "label": "CWND MAX",
                                    "mapping": "ldmp-cwndmax"
                                },
                                {
                                    "type": "input",
                                    "label": "ACK Timeout",
                                    "mapping": "ldmp-ackto"
                                },
                                {
                                    "type": "input",
                                    "label": "Send Timeout",
                                    "mapping": "ldmp-sndto"
                                },
                                {
                                    "type": "input",
                                    "label": "RTT Multiplier",
                                    "mapping": "ldmp-rttm"
                                },
                                {
                                    "type": "input",
                                    "label": "Pre-buffer for packet delay (jitter)",
                                    "mapping": "ldmp-jitter2p"
                                },
                                {
                                    "type": "input",
                                    "label": "Shaping Percent",
                                    "mapping": "ldmp-maxbrt"
                                },
                                {
                                    "type": "button",
                                    "action": "submitLocalForm",
                                    "size": "small",
                                    "label": "Apply Changes"
                                }
                            ]
                        },
                        {
                            "title": "Target Decoder",
                            "apiSrc": ["/REST/encoder/network", "/REST/encoder/status"],
                            "postEndpoint": "/REST/encoder/network",
                            "backgroundColor": "#FEF1E9",
                            "type": "form",
                            "fields": [
                                {
                                    "type": "input",
                                    "label": "Destination IP",
                                    "mapping": "decoderIP"
                                },
                                {
                                    "type": "input",
                                    "label": "Destination Port",
                                    "mapping": "port",
                                    "endLabel": "UDP (default: 1770)"
                                },
                                {
                                    "type": "input",
                                    "label": "IP Packet Size",
                                    "mapping": "packetsize",
                                    "endLabel": "bytes (default: 1392)"
                                },
                                {
                                    "type": "button",
                                    "action": "submitLocalForm",
                                    "size": "small",
                                    "label": "Apply Changes"
                                }
                            ]
                        },
                        {
                            "title": "Forward Error Correction",
                            "apiSrc": "/REST/encoder/network",
                            "postEndpoint": "/REST/encoder/network",
                            "backgroundColor": "#FEF5CA",
                            "type": "form",
                            "fields": [
                                {
                                    "type": "select",
                                    "label": "FEC",
                                    "mapping": "FEC"
                                },
                                {
                                    "type": "select",
                                    "label": "Shuffle",
                                    "mapping": "shuffle"
                                },
                                {
                                    "type": "button",
                                    "action": "submitLocalForm",
                                    "size": "small",
                                    "label": "Apply Changes"
                                }
                            ]
                        }
                    ]
                },
                {
                    "routeName": "Config",
                    "containers": [
                        {
                            "title": "Video Settings",
                            "apiSrc": "/REST/encoder/video",
                            "postEndpoint": "/REST/encoder/video",
                            "type": "form",
                            "backgroundColor": "#FFEFD5",
                            "fields": [
                                {
                                    "type": "select",
                                    "label": "Video Resolution",
                                    "mapping": "Resolution"
                                },
                                {
                                    "type": "select",
                                    "label": "Color Profile",
                                    "mapping": "ColorSpace",
                                    "endLabel": "(default: 4:2:2)"
                                },
                                {
                                    "type": "select",
                                    "label": "Framerate",
                                    "mapping": "FrameRate",
                                    "endLabel": "(default: Full)"
                                },
                                {
                                    "type": "input",
                                    "label": "Key Frames",
                                    "mapping": "KeyFrame",
                                    "endLabel": "(default: 300 frames)"
                                    
                                },
                                {
                                    "type": "checkbox",
                                    "label": "Advanced Profile",
                                    "mapping": "RecycleIntra",
                                    "endLabel": "(Default: Off)"
                                },
                                {
                                    "type": "select",
                                    "label": "Bit Depth",
                                    "mapping": "BitDepth",
                                    "endLabel": "(Default: 10-bit)"
                                },
                                {
                                    "type": "select",
                                    "label": "Color Space",
                                    "mapping": "NativeColorSpace",
                                    "endLabel": "For RGB 4:4:4 only"
                                },
                                {
                                    "type": "input",
                                    "label": "Dynamic Resolution Quality",
                                    "mapping": "DynamicRes",
                                    "endLabel": "set to 0 to turn off"
                                },
                                {
                                    "type": "button",
                                    "action": "submitLocalForm",
                                    "size": "small",
                                    "label": "Apply Changes"
                                }
                            ]
                        },
                        {
                            "title": "Audio Settings",
                            "apiSrc": "/REST/encoder/audio",
                            "type": "form",
                            "backgroundColor": "#FFC1C1",
                            "postEndpoint": "/REST/encoder/audio",
                            "fields": [
                                {
                                    "type": "select",
                                    "label": "Codec",
                                    "mapping": "AudioCodec",
                                    "endLabel": "(default: AAC)"
                                },
                                {
                                    "type": "select",
                                    "label": "Channels",
                                    "mapping": "AudioChannels",
                                    "endLabel": "(default: 2-ch)"
                                },
                                {
                                    "type": "select",
                                    "label": "Sample Rate",
                                    "mapping": "SampleRate",
                                    "endLabel": "(default: 48000 Hz)"
                                },
                                {
                                    "type": "button",
                                    "action": "submitLocalForm",
                                    "size": "small",
                                    "label": "Apply Changes"
                                }
                            ]
                        }
                    ]
                },
                {
                    "routeName": "Metadata",
                    "containers": [
                        {
                            "title": "Metadata Settings",
                            "apiSrc": "/REST/encoder/metadata",
                            "type": "form",
                            "backgroundColor": "#FFE4E1",
                            "postEndpoint": "/REST/encoder/metadata",
                            "fields": [
                                {
                                    "type": "input",
                                    "label": "Title",
                                    "mapping": "Meta_Slug"
                                },
                                {
                                    "type": "input",
                                    "label": "Location",
                                    "mapping": "Meta_Location"
                                },
                                {
                                    "type": "input",
                                    "label": "Reporter",
                                    "mapping": "Meta_Reporter"
                                },
                                {
                                    "type": "input",
                                    "label": "Producer",
                                    "mapping": "Meta_Producer"
                                },
                                {
                                    "type": "input",
                                    "label": "DRM",
                                    "mapping": "Meta_Network1"
                                },
                                {
                                    "type": "button",
                                    "action": "submitLocalForm",
                                    "size": "small",
                                    "label": "Apply Changes"
                                }
                            ]
                        }
                    ]
                }
            ]
        }
    }
}